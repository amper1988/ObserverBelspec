<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.layout.*?>
<AnchorPane opacity="0.8" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ControllerFormLoading">
    <children>
        <GridPane alignment="CENTER" hgap="10.0" prefHeight="400.0" prefWidth="400.0" vgap="10.0"
                  AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                  AnchorPane.topAnchor="0.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <ProgressIndicator fx:id="piProgress" opacity="0.11">
                    <GridPane.margin>
                        <Insets bottom="25.0" left="25.0" right="25.0" top="25.0"/>
                    </GridPane.margin>
                </ProgressIndicator>
                <Label fx:id="lblInfo" alignment="CENTER" text="Label" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets bottom="25.0" left="185.0" right="25.0" top="25.0"/>
                    </GridPane.margin>
                </Label>
            </children>
        </GridPane>
    </children>
</AnchorPane>
