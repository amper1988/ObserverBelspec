<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<GridPane hgap="10.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" vgap="10.0"
          xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="ControllerFormUncheckArrest">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="200.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <Label text="Введите основание для снятия ареста" GridPane.columnSpan="2">
            <GridPane.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </GridPane.margin>
        </Label>
        <Label text="Основание для снятия ареста" GridPane.rowIndex="1">
            <GridPane.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </GridPane.margin>
        </Label>
        <TextField fx:id="txtUncheckReason" minWidth="250.0" promptText="Наименование документа, дата"
                   GridPane.columnIndex="1" GridPane.rowIndex="1">
            <GridPane.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </GridPane.margin>
        </TextField>
        <Button fx:id="btnUncheckArrest" mnemonicParsing="false" text="Снять арест" GridPane.columnIndex="1"
                GridPane.rowIndex="2">
            <GridPane.margin>
                <Insets bottom="5.0" left="195.0" right="5.0" top="5.0"/>
            </GridPane.margin>
        </Button>
    </children>
</GridPane>
